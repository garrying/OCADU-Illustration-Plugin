{
  "name": "garrying/ocaduillustration",
  "type": "package",
  "description": "A Wordpress thing for OCAD U Illustration Program.",
  "require": {
    "php": ">=7.4"
  },
  "require-dev": {
    "wp-coding-standards/wpcs": "^2.3",
    "php-parallel-lint/php-parallel-lint": "^1.3",
    "dealerdirect/phpcodesniffer-composer-installer": "^0.7.1"
  },
  "scripts": {
    "checkcs": "@php ./vendor/squizlabs/php_codesniffer/bin/phpcs --runtime-set testVersion 7.4-",
    "fixcs": "@php ./vendor/squizlabs/php_codesniffer/bin/phpcbf --runtime-set testVersion 7.4-",
    "lint": "@php ./vendor/bin/parallel-lint --exclude .git --exclude vendor ."
  },
  "license": "MIT"
}